<div class="title text-center m-2"><h3>Add new list</h3></div>

<div class="container-form d-flex justify-content-center text-center">
  <div class="form-new-list">
    <%= simple_form_for(@list) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit %>
    <% end %>
    <a href=<%=lists_path%> class="btn btn-dark text-center">Back</a>
  </div>
</div>
